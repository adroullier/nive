<tal:block metal:define-macro="main"><!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="de" lang="de" i18n:domain="poolyx">
<head>
<title tal:content="view.HtmlTitle()"></title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
<meta name="generator" content="PoolyxCMS 0.6" />

<script tal:attributes="src view.StaticUrl('Poolyx.cms.cmsview:static/mods/jquery-1.6.4.min.js')" type="text/javascript"></script>
<link tal:attributes="href view.StaticUrl('Poolyx.cms.cmsview:static/poolyx.css')" rel="stylesheet" type="text/css" media="all" />
<tal:block metal:define-macro="editorHead">
<link tal:attributes="href view.StaticUrl('Poolyx.cms.cmsview:static/toolbox/toolbox.css')" rel="stylesheet" type="text/css" media="all" />
<script tal:attributes="src view.StaticUrl('Poolyx.cms.cmsview:static/mods/ui/jquery-ui-1.8.12.custom.min.js')" type="text/javascript"></script>
<link tal:attributes="href view.StaticUrl('Poolyx.cms.cmsview:static/overlay/overlay.css')" rel="stylesheet" type="text/css" media="all" />
<script tal:attributes="src view.StaticUrl('Poolyx.cms.cmsview:static/overlay/overlay.js')" type="text/javascript"></script>
<link tal:attributes="href view.StaticUrl('Poolyx.cms.cmsview:static/pe.css')" rel="stylesheet" type="text/css" media="all" />
<script tal:attributes="src view.StaticUrl('Poolyx.cms.cmsview:static/pe.js')" type="text/javascript"></script>
</tal:block>
<tal:block metal:define-slot="head"></tal:block>
<tal:block tal:condition="head|None" tal:replace="structure head" tal:on-error="nothing"></tal:block>
</head>

<body>
<div id="container">
<a name="top"></a>
<header>
<div id="logo">Poolyx</div>
</header>
<nav>
<div class="spacer"> </div>
<a tal:repeat="tab view.getEditorWidgets(context)"
   tal:content="structure tab.name"
   tal:attributes="href tab.viewmapper; class 'tab active'+str(request.view_name==tab.viewmapper)"></a>
</nav>
<div id="breadcrumbs">
  <span tal:content="context.GetObjectName()" i18n:translate=""/> -> <span tal:replace="structure view.breadcrumbs(addHome=1,link=False)"/>
</div>
<div id="main">
<div id="poolyx_edit">

  <div tal:condition="request.session.peek_flash()" class="alert alert-sucess">
    <ul><li tal:repeat="msg request.session.pop_flash()"
            tal:content="msg"></li></ul>
  </div>

  <tal:block metal:define-slot="editmain"></tal:block>
</div>
</div>
 
</div>

</body>
</html>
</tal:block>
